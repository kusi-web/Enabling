<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_distribute_expense_form" model="ir.ui.view">
            <field name="name">distribute.expense.form</field>
            <field name="model">distribute.expense</field>
            <field name="arch" type="xml">
                <form string="Distribute Expenses">
                    <group name="group1">
                        <field name="vendor_bill_amount"/>
                        <field name="label"/>
                        <field name="reconciliation_model"/>
                    </group>
                    <footer>
                        <button string="Confirm" name="action_confirm" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>
